<?xml version="1.0" encoding="UTF-8" ?>
<selector>
    <item pressed="true">
        <shape
            type="rounded"
            radius="4dp"
            color="#000"/>
    </item>
    <item hovered="true">
        <shape
            type="rounded"
            radius="4dp"
            color="?primaryColor"/>
    </item>
    <item>
        <shape
            type="rounded"
            radius="4dp"
            color="?colorSurfaceSecondary"/>
    </item>
</selector>